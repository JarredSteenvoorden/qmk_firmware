// Escape, Tab, Enter, Backspace, Del
COMB(esc, KC_ESC, KC_W, KC_E)
COMB(tab, KC_TAB, KC_S, KC_D)

COMB(enter, KC_ENT, KC_J, KC_I)
COMB(enter_num, KC_ENT, KC_MINS, KC_8)
COMB(enter_nav, KC_ENT, KC_LEFT, KC_UP)

COMB(bspace, KC_BSPC, KC_I, KC_O)
COMB(bspace_num, KC_BSPC, KC_8, KC_9)
COMB(bspace_nav, KC_BSPC, KC_UP, KC_END)

COMB(delete, KC_DEL, KC_K, KC_L)
COMB(delete_num, KC_DEL, KC_EQL, KC_LBRC)
COMB(delete_nav, KC_DEL, KC_DOWN, KC_RGHT)

// Symbols
COMB(slsh, KC_SLSH, KC_COMM, KC_DOT)

// One shot CTRL, ALT, GUI
COMB(osm_lalt, OSM(MOD_LALT), KC_W, KC_R)     // Requires PR #8591
COMB(osm_lctrl, OSM(MOD_LCTL), KC_S, KC_F)    // Requires PR #8591
COMB(osm_lgui, OSM(MOD_LGUI), KC_X, KC_V)     // Requires PR #8591

COMB(osm_ralt, OSM(MOD_RALT), KC_U, KC_O)     // Requires PR #8591
COMB(osm_rctrl, OSM(MOD_RCTL), KC_J, KC_L)    // Requires PR #8591
COMB(osm_rgui, OSM(MOD_RGUI), KC_M, KC_DOT)   // Requires PR #8591

// Cut, Copy, Paste
SUBS(cut,SS_LCTRL("x"),KC_X, KC_C, KC_V)      // Requires PR #8591
SUBS(copy,SS_LCTRL("c"), KC_X, KC_C)
SUBS(paste,SS_LCTRL("v"), KC_C, KC_V)
