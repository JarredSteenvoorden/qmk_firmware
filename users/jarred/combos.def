// One shot CTRL, ALT, GUI
COMB(osm_lgui, OSM(MOD_LGUI), KC_W, KC_R)     // Requires PR #8591
COMB(osm_lctrl, OSM(MOD_LCTL), KC_S, KC_F)    // Requires PR #8591
COMB(osm_rctrl, OSM(MOD_RCTL), KC_J, KC_L)    // Requires PR #8591
COMB(osm_ralt, OSM(MOD_RALT), KC_U, KC_O)     // Requires PR #8591

// Cut, Copy, Paste
SUBS(cut,SS_LCTRL("x"),KC_X, KC_C, KC_V)      // Requires PR #8591
SUBS(copy,SS_LCTRL("c"), KC_X, KC_C)
SUBS(paste,SS_LCTRL("v"), KC_C, KC_V)
